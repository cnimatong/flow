{
  "expo": {
    "name": "flow1.0",
    "slug": "flow1-0",
    "version": "1.0.0",
    "scheme": "flow1.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flow.firebase",
      "googleServicesFile": "./Firebase Auth Config.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flow.firebase",
      "googleServicesFile": "MADFinal Firebase config.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-build-properties", {
        "ios": {
          "useFrameworks": "static"
        }
      }],
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ]
  }
}
